    "clustering",
    "geospatial",
    "mbtiles"
  ],
  "dependencies": {
    "esm": "^3.1.0",
    "node-gzip": "^1.1.2",
    "sqlite": "^3.0.3",
    "sqlite3": "^4.1.0",
    "supercluster": "^6.0.1",
    "vt-pbf": "^3.1.1",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-mourner": "^3.0.0",
    "tap": "^12.1.1"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "camelcase": 0
    }
  },
  "bugs": {
    "url": "https://github.com/majda107/supertiler/issues"
  },
  "homepage": "https://github.com/majda107/supertiler#readme",
